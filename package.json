    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "postcss": "^8.4.40",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.4.2",
    "ts-node": "^8",
    "tsx": "^3.14.0",
    "typescript": "~5.6.2",
